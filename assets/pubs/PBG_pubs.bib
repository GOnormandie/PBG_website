	year = {2019},
	note = {00000},
	pages = {71--77},
	file = {Clarke et al. - 2019 - Clipping the tail fin enables cohort identificatio.pdf:/Users/phillipsb/Zotero/storage/AKI2YKPB/Clarke et al. - 2019 - Clipping the tail fin enables cohort identificatio.pdf:application/pdf},
