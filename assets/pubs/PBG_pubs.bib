@article{macdonald2017,
	title = {Peripheral {Isolates} as {Sources} of {Adaptive} {Diversity} under {Climate} {Change}},
	volume = {5},
	copyright = {All rights reserved},
	issn = {2296-701X},
	url = {http://journal.frontiersin.org/article/10.3389/fevo.2017.00088/full?&utm_source=Email_to_authors_&utm_medium=Email&utm_content=T1_11.5e1_author&utm_campaign=Email_publication&field=&journalName=Frontiers_in_Ecology_and_Evolution&id=272170},
	doi = {10.3389/fevo.2017.00088},
	abstract = {As climate change progresses, there is increasing focus on the possibility of using targeted gene flow (the movement of pre-adapted individuals into declining populations) as a management tool. Targeted gene flow is a relatively cheap, low-risk management option and will almost certainly come into increased use over the coming decades. Before such action can be taken, however, we need to know where to find pre-adapted individuals. We argue that, for many species, the obvious place to look for this diversity is in peripheral isolates: isolated populations at the current edges of a species’ range. Both evolutionary and ecological considerations suggest that the bulk of a species’ adaptive variation may be contained in the total set of these peripheral isolates. Moreover, by exploring both evolutionary and ecological perspectives it becomes clear that we should be able to assess the potential value of each isolate using remotely sensed data and three measurable axes of variation in patch traits: population size, connectivity, and climatic environment. Locating the “sweet spot” in this trait space, however, remains a challenge. Throughout, we illustrate these ideas using Australia’s Wet Tropics rainforests as a model system.},
	language = {English},
	journal = {Frontiers in Ecology and Evolution},
	author = {Macdonald, Stewart L. and Llewelyn, John and Moritz, Craig and Phillips, Ben L.},
	year = {2017},
	note = {00000},
	keywords = {Assisted gene flow, Climate Change, geographic variation, local adaptation, targeted gene flow},
	pages = {88},
	file = {Macdonald et al. - 2017 - Peripheral Isolates as Sources of Adaptive Diversi.pdf:/Users/phillipsb/Zotero/storage/4U73RVAK/Macdonald et al. - 2017 - Peripheral Isolates as Sources of Adaptive Diversi.pdf:application/pdf},
}

