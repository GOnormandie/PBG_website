@article{burns2020,
	title = {Time since fire is an over-simplified measure of habitat suitability for the {New} {Holland} mouse},
	volume = {101},
	copyright = {All rights reserved},
	issn = {0022-2372, 1545-1542},
	url = {https://academic.oup.com/jmammal/article/101/2/476/5637236},
	doi = {10.1093/jmammal/gyz157},
	abstract = {Abstract
            Fire has shaped much of the Australian landscape, and alterations to natural or historical fire regimes are implicated in the decline of many native mammal species. Time since fire (TSF) is a common metric used to understand vegetation and faunal responses to fire but is unlikely to capture the complexity of successional changes following fire. The New Holland mouse (Pseudomys novaehollandiae), a threatened and declining rodent species native to southeastern Australia, is traditionally considered an early post-fire successional species. Here, we use a 48-year dataset to test whether this posited association with early TSF is upheld, and whether the species’ occurrence and abundance are governed by TSF. We find support for a minimal influence of TSF on the species’ occurrence, and that while abundance of P. novaehollandiae is partly explained by TSF, considerable uncertainty and variation among fire events and locations limit the usefulness of TSF in informing conservation management strategies. We suggest that it is not helpful to consider the species as early successional and that fire planning for P. novaehollandiae conservation is best considered at a local scale. Additionally, we provide guidelines for maximizing individual survival and persistence during and after planned burns.},
	language = {en},
	number = {2},
	urldate = {2020-11-30},
	journal = {Journal of Mammalogy},
	author = {Burns, Phoebe A and Phillips, Ben L},
	editor = {Pavey, Chris},
	month = may,
	year = {2020},
	note = {00000},
	pages = {476--486},
	file = {Burns and Phillips - 2020 - Time since fire is an over-simplified measure of h.pdf:/Users/phillipsb/Zotero/storage/LZCNFECY/Burns and Phillips - 2020 - Time since fire is an over-simplified measure of h.pdf:application/pdf},
}

@article{burns2019,
	title = {Accounting for detectability and abundance in survey design for a declining species},
	volume = {25},
	copyright = {All rights reserved},
	issn = {13669516},
	url = {http://doi.wiley.com/10.1111/ddi.12966},
	doi = {10.1111/ddi.12966},
	abstract = {Aim: As populations decline they become harder to detect simply because there are fewer animals to observe. In such cases, survey effort that was once reliable may become inadequate and the species incorrectly labelled locally extinct, causing management efforts to cease. For declining populations, then, we need to determine the survey effort necessary to detect the species, assuming only a single individual is present at a site. Detectability, however, often varies wildly with seasonal, behavioural and environmental variables. Thus, we need statistical analyses and survey design which can deal with highly variable detection rates and allow us to detect even very small populations. This is especially true in Australia, where many mammal species are experiencing serious decline. Here, we examine a threatened Australian rodent, the New Holland Mouse (Pseudomys novaehollandiae): we determine minimum survey requirements for reliable detection and show that historical survey effort is inadequate to assert local extinction.},
	language = {en},
	number = {10},
	urldate = {2019-09-15},
	journal = {Diversity and Distributions},
	author = {Burns, Phoebe A. and McCall, Claire and Rowe, Kevin C. and Parrott, Marissa L. and Phillips, Ben L.},
	month = oct,
	year = {2019},
	note = {00000},
	pages = {1655--1665},
	file = {Burns et al. - 2019 - Accounting for detectability and abundance in surv.pdf:/Users/phillipsb/Zotero/storage/ZYS8ZASC/Burns et al. - 2019 - Accounting for detectability and abundance in surv.pdf:application/pdf},
}

@article{burns2018,
	title = {Identification of threatened rodent species using infrared and white-flash camera traps},
	volume = {40},
	copyright = {All rights reserved},
	issn = {0310-0049},
	url = {http://www.publish.csiro.au/?paper=AM17016},
	doi = {10.1071/AM17016},
	abstract = {Camera trapping has evolved into an efﬁcient technique for gathering presence/absence data for many species; however, smaller mammals such as rodents are often difﬁcult to identify in images. Identiﬁcation is inhibited by cooccurrence with similar-sized small mammal species and by camera set-ups that do not provide adequate image quality. Here we describe survey procedures for identiﬁcation of two small, threatened rodent species – smoky mouse (Pseudomys fumeus) and New Holland mouse (P. novaehollandiae) – using white-ﬂash and infrared camera traps. We tested whether observers could accurately identify each species and whether experience with small mammals inﬂuenced accuracy. Pseudomys fumeus was {\textasciitilde}20 times less likely to be misidentiﬁed on white-ﬂash images than infrared, and observer experience affected accuracy only for infrared images, where it accounted for all observer variance. Misidentiﬁcations of P. novaehollandiae were more common across both ﬂash types: false positives ({\textgreater}0.21) were more common than false negatives ({\textless}0.09), and experience accounted for only 31\% of variance in observer accuracy. For this species, accurate identiﬁcation appears to be, in part, an innate skill. Nonetheless, using an appropriate setup, camera trapping clearly has potential to provide broad-scale occurrence data for these and other small mammal species.},
	language = {en},
	number = {2},
	urldate = {2019-02-06},
	journal = {Australian Mammalogy},
	author = {Burns, Phoebe A. and Parrott, Marissa L. and Rowe, Kevin C. and Phillips, Benjamin L.},
	year = {2018},
	note = {00000},
	pages = {188--197},
	file = {AM17016.pdf:/Users/phillipsb/Zotero/storage/9FE3GM6E/AM17016.pdf:application/pdf;Burns et al. - 2018 - Identification of threatened rodent species using:/Users/phillipsb/Zotero/storage/T3D6CCKT/Burns et al. - 2018 - Identification of threatened rodent species using:application/pdf},
